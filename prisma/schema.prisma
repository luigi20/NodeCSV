// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Products{
  id String @id @default(uuid())
  codeBar String 
  description String
  price Float 
  quantity Int

@@map("products")
}